#!/bin/sh /etc/rc.common

USE_PROCD=1

start_service() {
    procd_open_instance
    procd_set_param command /usr/bin/sender
    procd_append_param command --orgId=gut697 --typeId=RUTX10 --deviceId=RUTX10_Dom --auth='!P(oEdp2@w2F-*cBtB'
    procd_set_param pidfile /var/run/sender.pid
    procd_close_instance
}